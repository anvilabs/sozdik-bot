[ignore]
.*/node_modules/.*/__test__/
.*/node_modules/.*/__tests__/
.*/node_modules/.*/test/
.*/node_modules/.*/tests/
<PROJECT_ROOT>/build/

[libs]
flow-typed/

[lints]
sketchy-null=error
untyped-type-import=error

[options]
emoji=true

experimental.const_params=true

module.name_mapper='^src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'

munge_underscores=true

[version]
^0.52.0
